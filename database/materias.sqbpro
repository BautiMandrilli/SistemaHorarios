CREATE TABLE MateriaXCarrera(
    idMateria TEXT,
    idCarrera TEXT,
    PRIMARY KEY(idMateria, idCarrera),
    FOREIGN KEY(idMateria) REFERENCES Materias(id),
    FOREIGN KEY(idCarrera) REFERENCES Carrera(id)
);

CREATE TABLE MateriaXCorrelativa(
    idMateriaAInscribir TEXT,
    idMateriaCorrelativa TEXT,
    idTipoCorrelativa INTEGER NOT NULL,
    PRIMARY KEY(idMateriaAInscribir, idMateriaCorrelativa),
    FOREIGN KEY(idMateriaAInscribir) REFERENCES Materias(id),
    FOREIGN KEY(idMateriaCorrelativa) REFERENCES Materias(id),
    FOREIGN KEY(idTipoCorrelativa) REFERENCES TipoCorrelativa(id)
);

CREATE TABLE MatXCursoXTipoCuatXClase (
    idMateria TEXT,
    idCurso TEXT,
    idTipoCuatri INTEGER,
    idFechaClase INTEGER,
    horaInicio TEXT NOT NULL,
    horaFin TEXT NOT NULL,
    calsificacion TEXT,
    PRIMARY KEY(idMateria, idCurso, idTipoCuatri, idFechaClase),
    FOREIGN KEY(idMateria) REFERENCES Materias(id),
    FOREIGN KEY(idCurso) REFERENCES Cursos(id),
    FOREIGN KEY(idTipoCuatri) REFERENCES TipoCuatrimestre(id),
    FOREIGN KEY(idFechaClase) REFERENCES FechaClase(id)
);

