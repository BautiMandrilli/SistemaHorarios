    id TEXT PRIMARY KEY,
    name TEXT NOT NULL,
	acronimo TEXT NOT NULL UNIQUE, 
    anioCurso INTEGER NOT NULL 
);

CREATE TABLE Curso (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL
);

CREATE TABLE materiaXcurso (
    idMateria TEXT,
    idCurso INTEGER NOT NULL,
    fechaInicio TEXT NOT NULL,
    fechaFin TEXT NOT NULL,
    PRIMARY KEY(idMateria, idCurso),
    FOREIGN KEY (idMateria) REFERENCES Materias(id),
    FOREIGN KEY (idCurso) REFERENCES Curso(id)
);
